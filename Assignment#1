    "import random\n",
    "\n",
    "emotions = [\n",
    "    'raging',\n",
    "    'frustrating', \n",
    "    'menacing', \n",
    "    'annoying', \n",
    "    'aggravating', \n",
    "    'irritating', \n",
    "    'anxious', \n",
    "    'edgy', \n",
    "    'nervous', \n",
    "    'worried',\n",
    "    'tense'\n",
    "]\n",
