    "people = [\n",
    "    'doctor', \n",
    "    'nurse', \n",
    "    'radiologist',\n",
    "    'security',\n",
    "    'student health center', \n",
    "    'insurance help',''\n",
    "]\n",
    "\n",
    "verbs = [\n",
    "    'complaining', \n",
    "    'wailing', \n",
    "    'crying', \n",
    "    'beefing', \n",
    "    'screaming', \n",
    "    'muttering', \n",
    "    'whimpering', 'whining']\n",
